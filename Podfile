# Uncomment this line to define a global platform for your project
platform :ios, ‘8.0’
# Uncomment this line if you're using Swift
use_frameworks!

def common_pods
    pod 'FDKeychain', '~> 1.2'
    pod 'Alamofire', '~> 3.2'
    pod 'JTCalendar', '~> 2.0'
    pod 'STPopup'
    pod 'TPKeyboardAvoiding'
    pod 'EVReflection', '~> 2.33'
end

target 'OneSecApp' do
     common_pods
end

target 'OneSecAppTests' do
     common_pods
end

target 'OneSecAppUITests' do
     common_pods
end

